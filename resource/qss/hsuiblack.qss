}
*/
QPushButton:hover{
    padding-top: -15px;
    padding-bottom: -17px;
}

QPushButton:pressed {
    background-color: #426ab3;
    padding-top: -15px;
    padding-bottom: -17px;
    border: 1px solid #d93a49;
    border-color: #4e72b8;
}

QPushButton::menu-indicator {
    subcontrol-origin: padding;
    subcontrol-position: bottom right;
    left: 8px;
}

/**********************************************/

/******************QToolButton*******************/

QToolButton {
    background-color: transparent;
    border: 1px transparent #77787b;
    border-radius: 2px;
    margin: 0px;
    padding: 0px;
}

QToolButton#qt_toolbar_ext_button {
    background: #58595a
}

QToolButton[popupMode="1"] {
    /* only for MenuButtonPopup */
    padding-right: 20px;
    /* make way for the popup button */
    border: 1px #77787b;
    border-radius: 5px;
}

QToolButton[popupMode="2"] {
    /* only for InstantPopup */
    padding-right: 10px;
    /* make way for the popup button */
    border: 1px #77787b;
}

QToolButton:hover,
QToolButton::menu-button:hover {
    background-color: transparent;
    border: 1px solid #2a5caa;
    padding: 1px;
}

QToolButton:checked,
QToolButton:pressed,
QToolButton::menu-button:pressed {
    background-color: #2a5caa;
    border: 1px solid #2a5caa;
    padding: 1px;
}


/* the subcontrol below is used only in the InstantPopup or DelayedPopup mode */

QToolButton::menu-indicator {
    image: url(:/qss_icons/resources/down_arrow.png);
    top: -2px;
    left: -2px;
    /* shift it a bit */
}


/* the subcontrols below are used only in the MenuButtonPopup mode */

QToolButton::menu-button {
    border: 1px transparent #77787b;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
    /* 16px width + 4px for border = 20px allocated above */
    width: 16px;
    outline: none;
}

QToolButton::menu-arrow {
    image: url(:/qss_icons/resources/down_arrow.png);
}

QToolButton::menu-arrow:open {
    border: 1px solid #77787b;
}

QToolButton::right-arrow:enabled {
    image: url(:/qss_icons/resources/right_arrow.png);
}

QToolButton::left-arrow:enabled {
    image: url(:/qss_icons/resources/left_arrow.png);
}

QToolButton::right-arrow:disabled {
    image: url(:/qss_icons/resources/right_arrow_disabled.png);
}

QToolButton::left-arrow:disabled {
    image: url(:/qss_icons/resources/left_arrow_disabled.png);
}

