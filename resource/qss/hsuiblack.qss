/**********************************************/

/******************QTextEdit******************/

QTextEdit {
    background-color: #fffffb;
    color: #77787b;
    border: 1px solid #77787b;
}

QTextEdit:disabled {
    background-color: #8a8c8e;
    color: #77787b;
}

/********************************************/

/******************QPlainTextEdit******************/

QPlainTextEdit {
    background-color: #fffffb;
    color: #77787b;
    border-radius: 2px;
    border: 1px solid #77787b;
}

/**********************************************/

/******************QSizeGrip******************/

QSizeGrip {
    image: url(:/qss_icons/resources/sizegrip.png);
    width: 12px;
    height: 12px;
}

/**********************************************/

/******************QMainWindow******************/

QMainWindow::separator {
    background-color: #31363b;
    color: white;
    padding-left: 4px;
    spacing: 2px;
    border: 1px dashed #77787b;
}

QMainWindow::separator:hover {
    background-color: #787876;
    color: white;
    padding-left: 4px;
    border: 1px solid #77787b;
    spacing: 2px;
}

