/**********************************************/

/******************QDateEdit*******************/

QDateEdit {
    selection-background-color: #fffffb;
    border-style: solid;
    border: 1px solid #77787b;
    border-radius: 2px;
    padding: 5px;
}

QDateEdit:on {
    padding-top: 3px;
    padding-left: 4px;
    selection-background-color: #4a4a4a;
}

QDateEdit QAbstractItemView {
    background-color: #fffffb;
    border-radius: 2px;
    border: 1px solid #77787b;
    selection-background-color: #2a5caa;
}

QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-left-width: 0px;
    border-left-color: darkgray;
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

QDateEdit::down-arrow {
   image: url(:/qss_icons/resources/down_arrow_disabled.png);
}

QDateEdit::down-arrow:on,
QDateEdit::down-arrow:hover,
QDateEdit::down-arrow:focus {
    image: url(:/qss_icons/resources/down_arrow.png);
}

/**********************************************/

/******************QSlider*******************/

QSlider::groove:horizontal {
    border: 1px solid #77787b;
    height: 4px;
    background: #f6f5ec;
    margin: 0px;
    border-radius: 2px;
}

QSlider::handle:horizontal {
    background: #fffffb;
    border: 1px solid #77787b;
    width: 16px;
    height: 16px;
    margin: -8px 0;
    border-radius: 9px;
}

QSlider::sub-page:horizontal {
    border: 1px solid #565a5e;
    height: 4px;
    background: #2a5caa;
    margin: 0px;
    border-radius: 2px;
}

QSlider::groove:vertical {
    border: 1px solid #77787b;
    width: 4px;
    background: #f6f5ec;
    margin: 0px;
    border-radius: 3px;
}

QSlider::handle:vertical {
    background: #fffffb;
    border: 1px solid #77787b;
    width: 16px;
    height: 16px;
    margin: 0 -8px;
    border-radius: 9px;
}

QSlider::sub-page:vertical {
    border: 1px solid #565a5e;
    width: 4px;
    background: #2a5caa;
    margin: 0px;
    border-radius: 3px;
}

QSlider:focus {
    border: none;
}

