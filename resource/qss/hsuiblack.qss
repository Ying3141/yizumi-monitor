/**********************************************/

/******************QGroupBox*******************/

QGroupBox::indicator {
    width: 18px;
    height: 18px;
}

QGroupBox::indicator {
    margin-left: 2px;
}

QGroupBox::indicator:unchecked {
    image: url(:/qss_icons/resources/checkbox_unchecked.png);
}

QGroupBox::indicator:unchecked:hover,
QGroupBox::indicator:unchecked:focus,
QGroupBox::indicator:unchecked:pressed {
    border: none;
    image: url(:/qss_icons/resources/checkbox_unchecked_focus.png);
}

QGroupBox::indicator:checked {
    image: url(:/qss_icons/resources/checkbox_checked.png);
}

QGroupBox::indicator:checked:hover,
QGroupBox::indicator:checked:focus,
QGroupBox::indicator:checked:pressed {
    border: none;
    image: url(:/qss_icons/resources/checkbox_checked_focus.png);
}

QGroupBox::indicator:checked:disabled {
    image: url(:/qss_icons/resources/checkbox_checked_disabled.png);
}

QGroupBox::indicator:unchecked:disabled {
    image: url(:/qss_icons/resources/checkbox_unchecked_disabled.png);
}

/**********************************************/

/******************QMenuBar*******************/

QMenuBar {
    background-color: #31363b;
    color: #eff0f1;
}

QMenuBar::item {
    background: transparent;
}

QMenuBar::item:selected {
    background: transparent;
    border: 1px solid #77787b;
}

QMenuBar::item:pressed {
    border: 1px solid #77787b;
    background-color: #2a5caa;
    color: #eff0f1;
    margin-bottom: -1px;
    padding-bottom: 1px;
}

QMenuBar:focus {
    border: 1px solid #2a5caa;
}

/**********************************************/

/******************QMenu*******************/

QMenu {
    border: 1px solid #77787b;
    color: #eff0f1;
    margin: 2px;
}

QMenu::icon {
    margin: 5px;
}

QMenu::item {
    padding: 5px 30px 5px 30px;
    border: 1px solid transparent;
    /* reserve space for selection border */
}

QMenu::item:selected {
    color: #eff0f1;
}

QMenu::separator {
    height: 2px;
    background: lightblue;
    margin-left: 10px;
    margin-right: 5px;
}

QMenu::indicator {
    width: 18px;
    height: 18px;
}


/* non-exclusive indicator = check box style indicator
   (see QActionGroup::setExclusive) */

QMenu::indicator:non-exclusive:unchecked {
    image: url(:/qss_icons/resources/checkbox_unchecked.png);
}

QMenu::indicator:non-exclusive:unchecked:selected {
    image: url(:/qss_icons/resources/checkbox_unchecked_disabled.png);
}

QMenu::indicator:non-exclusive:checked {
    image: url(:/qss_icons/resources/checkbox_checked.png);
}

QMenu::indicator:non-exclusive:checked:selected {
    image: url(:/qss_icons/resources/checkbox_checked_disabled.png);
}


/* exclusive indicator = radio button style indicator (see QActionGroup::setExclusive) */

QMenu::indicator:exclusive:unchecked {
    image: url(:/qss_icons/resources/radio_unchecked.png);
}

QMenu::indicator:exclusive:unchecked:selected {
    image: url(:/qss_icons/resources/radio_unchecked_disabled.png);
}

QMenu::indicator:exclusive:checked {
    image: url(:/qss_icons/resources/radio_checked.png);
}

QMenu::indicator:exclusive:checked:selected {
    image: url(:/qss_icons/resources/radio_checked_disabled.png);
}

QMenu::right-arrow {
    margin: 5px;
    image: url(:/qss_icons/resources/right_arrow.png)
}

QMenu::separator {
    height: 1px;
    background-color: #77787b;
    color: white;
    padding-left: 4px;
    margin-left: 10px;
    margin-right: 5px;
}

