QGroupBox {
    border: 1px solid #77787b;
    border-radius: 2px;
    margin-top: 20px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
}

/**********************************************/

/****************QAbstractScrollArea****************/

QAbstractScrollArea {
    border-radius: 2px;
    border: 1px solid #77787b;
    background-color: #fffffb;
}

